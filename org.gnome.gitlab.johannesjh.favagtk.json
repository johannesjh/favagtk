{
    "app-id" : "org.gnome.gitlab.johannesjh.favagtk",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "43",
    "sdk" : "org.gnome.Sdk",
    "command" : "favagtk",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--filesystem=host"
    ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        "requirements/blueprint-compiler.json",
        "requirements/python3-scikit-learn.json",
        "requirements/python3-main.json",
        "requirements/python3-optional.json",
        {
            "name" : "favagtk",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "dir",
                    "path" : "."
                }
            ],
            "config-opts" : [
                "-Dprofile=release"
            ]
        }
    ],
    "build-options" : {
        "env" : {        }
    }
}
